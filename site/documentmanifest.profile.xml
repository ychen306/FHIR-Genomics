        <max value="1"/>
        <type>
          <code value="Identifier"/>
        </type>
        <isModifier value="false"/>
        <mapping>
          <identity value="rim"/>
          <map value="id [need a list semantic to indicate order matters]"/>
        </mapping>
      </definition>
    </element>
    <element>
